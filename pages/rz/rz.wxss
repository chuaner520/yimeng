/* pages/rz/rz.wxss */
.header{
  width: 100%;
  height: 400rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cont{
  width: 90%;
  height: 100%;
  padding: 20rpx 0;
  margin-top: -50rpx;
}
.contn{
  width: 100%;
  margin-top: 50rpx;
}
.scann{
  font-weight: bold;
  color: #333;
}
.contn input{
  width: 95%;
  height: 60rpx;
  padding: 0 10px;
  border-radius: 10rpx;
  background-color: #f5f5f5;
  margin: 10rpx 0;
  font-size: 14px;
}

.flex-wrap{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.icon-guanbi.color-style {
  color: #000;
  font-size: 40rpx;
  position: absolute;
  right: 10%;
  top: 10%;
  font-size: 20rpx;
  color: rgb(65, 65, 65);
}
.icon-camera{
  color: #6C6F82;
  display: flex;
  justify-content: center;
  align-items: center;
}

.icon-camera, .flex-wrap image{
  font-size: 72rpx;
  border: 2rpx solid #eee;
  border-radius: 20rpx;
  width: 270rpx;
  height: 156rpx;
  text-align: center;
  line-height: 156rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
}
.flex-wrap .line{
  border: 1px solid white;
  max-width: 70%;
  max-height: 100%;
  margin-top: -10rpx;
}
.flex-wrap .relative{
  position: relative;
}
.scann text{
  font-size: 12px;
  color: gray;
}
.btn{
  background-color: royalblue;
  color: white;
  font-weight: 400;
  margin: 30rpx 0;
  box-shadow: 3px 3px 10px royalblue;
}