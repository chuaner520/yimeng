/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.zwnr{
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  color: gray;
}
.zwnr image{
  max-width: 100%;
  max-height: 100%;
  object-fit: cover;
}
.d_fomr{
  width: 60rpx;
  position: fixed;
  bottom: 5%;
  right: 5%;
  z-index: 1111111111;
}
.ad_top,.ad_add{
  width: 60rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 100%;
  margin-bottom: 10rpx;
}
.ad_top image,.ad_add image{
  width: 40rpx;
  height: 40rpx;
}
